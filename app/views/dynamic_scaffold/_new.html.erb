<%= form_with model: @record, url: './create' do |form| %>
  <%@manager.editor.form_fields.each do |elem|%>
    <% if elem.type? :hidden_field %>
      <%= elem.render(form) %>
    <% elsif elem.type? :check_boxes %>
      <div class="form-group">
        <%if elem.label?%>
          <label><%= elem.label %></label>
        <%end%>
        <div>
        <%= elem.render(form) do |cb|%>
          <div class="form-check">
          <%= cb.label class: 'form-check-label' do%>
            <%= cb.check_box class: 'form-check-input' %> <%= cb.text %> 
          <%end%>
          </div>
        <%end%>
        </div>
      </div> 
    <% else %>
      <div class="form-group">
        <label><%= elem.label %></label>
        <%= elem.render(form, 'form-control') %>
        <%if elem.notes?%>
          <div class="form-text text-muted"><%= elem.render_notes(form.object, self) %></div>
        <%end%>
      </div>
    <% end %>
  <% end %>
  <div>
    <a href="./" class="btn btn-outline-secondary btn-default">
      <%= dynamic_scaffold_icon :back %> <%= t('dynamic_scaffold.back') %>
    </a>
    <a href="./" class="btn btn-outline-warning btn-warning">
      <%= dynamic_scaffold_icon :save %> <%= t('dynamic_scaffold.save') %>
    </a>
  </div>
<% end %>
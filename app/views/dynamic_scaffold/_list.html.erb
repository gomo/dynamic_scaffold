<ul class="resplist resplist-striped">
  <%@manager.list.each do |record|%>
    <li class="resplist-row">
      <div class="resplist-items">
      <%@manager.displays.each do |disp|%>
        <div class="<%= classNames('resplist-item', disp.class_attr_value) %>"<%= disp.html_attr %>>
          <div class="resplist-label"><%= disp.label record %></div>
          <div class="resplist-value"><%= disp.value record %></div>
        </div>
      <%end%>
      </div>
      <div class="resplist-footer clearfix">
        <div class="float-right">
          <div class="btn-group">
            <a href="#" class="btn btn-primary"><%= dynamic_scaffold_config.edit_icon %> Edit</a>
          </div>
          <div class="btn-group">
            <button class="btn btn-secondary btn-defualt">
              <%= dynamic_scaffold_config.up_icon %>
            </button>
            <button class="btn btn-secondary btn-defualt">
              <%= dynamic_scaffold_config.down_icon %>
            </button>
          </div>
          <div class="btn-group">
            <button type="submit" class="btn btn-danger" name="submit_delete" value="1">
              <%= dynamic_scaffold_config.delete_icon %>
            </button>
          </div>
        </div>
      </div>
    </li>
  <%end%>
</ul>